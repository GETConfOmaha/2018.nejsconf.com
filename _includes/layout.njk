---
showregisterbutton: false
showfooter: true
showbooklet: true
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>GETconf 2019{% if title %}—{{ title }}{% endif %}</title>
{% set css %}{% include "theme.css" %}{% endset %}
{% set js %}{% include "script.js" %}{% endset %}
		<link rel="preload" href="/assets/img/earth-nobg-crop.png" as="image" />

		<link href="https://fonts.googleapis.com/css?family=PT+Mono" rel="stylesheet">
		<link href="https://fonts.googleapis.com/css?family=Audiowide" rel="stylesheet">

		<style>{{ css | cssmin | safe }}</style>
		<script>{{ js | jsmin | safe }}</script>
		<meta property="og:title" content="GETconf 2019{% if title %}—{{ title }}{% endif %}" />

		<meta name="description" content="{% if speaker.name %}{{ speaker.name }} is speaking at GET CONF! {% endif %}GET CONF is a series of events to amplify + celebrate the voices of women & gender expansive individuals in tech.">
		<meta name="twitter:description" property="og:description" content="{% if speaker.name %}{{ speaker.name }} is speaking at GET CONF! {% endif %}GET CONF is a series of events to amplify + celebrate the voices of women & gender expansive individuals in tech.">

		<meta name="twitter:url" property="og:url" content="https://2019.getconf.com{{ page.url }}">
		<meta name="twitter:image" property="og:image" content="https://2019.getconf.com{% if speaker.social_avatar_url %}{{ speaker.social_avatar_url }}{% else %}/assets/img/opengraph.png{% endif %}">
		<meta name="twitter:image:alt" property="og:image:alt" content="{% if speaker.name %}{{ speaker.name }} at {% endif %}GETconf 2019">

		<meta name="twitter:card" content="summary_large_image">
		<meta name="twitter:site" content="@GETConfOmaha">
		<meta name="twitter:creator" content="@GETConfOmaha">

		<meta property="og:type" content="website">
		<meta property="og:site_name" content="GETconf 2019">
		<meta property="og:locale" content="en_US">
		<meta property="og:image:width" content="1180">
		<meta property="og:image:height" content="590">

		<!-- Global site tag (gtag.js) - Google Analytics -->
		<script async src="https://www.googletagmanager.com/gtag/js?id=UA-126894718-1"></script>
		<script>
		window.dataLayer = window.dataLayer || [];
		function gtag(){dataLayer.push(arguments);}
		gtag('js', new Date());

		gtag('config', 'UA-126894718-1');
		</script>

	</head>
	<body{% if bodyclass %} class="{{ bodyclass }}"{% endif %}>
{% if skipHero -%}
{% else -%}
		<div class="sg-c">
			<div class="sg-component-full">
				{% include "components/hero.njk" %}
			</div>
		</div>
{% endif -%}

		{{ content | safe }}

{% if showfooter %}
		<footer>
			<div class="sg-c">
{% if showregisterbutton %}
				<div class="sg-component sg-component-full sg-component-dark">
					<h2 class="hero-register"><a target="_blank" href="https://www.eventbrite.com/e/getconf-gender-equity-in-tech-conference-registration-54242894959" class="btn-primary">Register <span>for GETconf</span></a></h2>
				</div>
{% endif %}
				<div class="sg-component">
					<p>©2019 <a target="_blank" href="https://twitter.com/GETConfOmaha/lists/getconf-2019-organizers/members">GETconf Organizers</a></p>
				</div>
			</div>
		</footer>
{% endif %}
{% set imgJs %}{% include "images.js" %}{% endset %}
		<script>{{ imgJs | jsmin | safe }}</script>
	</body>
</html>
